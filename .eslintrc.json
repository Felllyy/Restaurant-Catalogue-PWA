{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": 0,
        "no-underscore-dangle": 0,
        "no-param-reassign": 0,
        "no-restricted-globals": 0,
        "no-void": 0,
        "no-use-before-define": 0,
        "no-multi-assign": 0,
        "max-len": 0,
        "eqeqeq": 0,
        "no-console": 0,
        "no-constant-condition": 0,
        "no-fallthrough": 0,
        "no-loop-func": 0,
        "default-case": 0,
        "no-undef": 0,
        "func-names": 0,
        "prefer-destructuring": 0,
        "no-mixed-operators": 0,
        "operator-linebreak": 0,
        "no-plusplus": 0,
        "no-shadow": 0,
        "arrow-parens": 0,
        "no-sequences": 0,
        "no-unused-expressions": 0,
        "semi": 0,
        "new-cap": 0,
        "block-scoped-var": 0,
        "vars-on-top": 0,
        "no-var": 0,
        "consistent-return": 0
    }
}
